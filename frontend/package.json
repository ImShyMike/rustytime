{
	"name": "rustytime",
	"private": false,
	"version": "0.3.0",
	"license": "AGPL-3.0",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ImShyMike/rustytime.git"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint . && prettier --check .",
		"format": "prettier --write .",
		"pwa": "pwa-asset-generator static/favicon.svg static/pwa --background '#b4befe' --padding '10%' --icon-only --mstile --favicon --manifest static/manifest.webmanifest --index src/app.html --path-override /pwa"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.2",
		"@eslint/js": "^10.0.1",
		"@iconify-json/lucide": "^1.2.90",
		"@sveltejs/adapter-cloudflare": "^7.2.6",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^25.2.3",
		"@vite-pwa/sveltekit": "^1.1.0",
		"echarts": "^6.0.0",
		"eslint": "^10.0.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.15.0",
		"globals": "^17.3.0",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"svelte": "^5.50.1",
		"svelte-check": "^4.3.6",
		"svelte-echarts": "^1.0.0",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.55.0",
		"unplugin-icons": "^23.0.1",
		"vite": "^7.3.1",
		"@prgm/sveltekit-progress-bar": "^3.0.2"
	},
	"dependencies": {
		"@catppuccin/tailwindcss": "^1.0.0"
	}
}
